		"preview-android": "node -e 'require(\"./scripts\").previewAndroid()'",
		"lint": "node -e 'require(\"./scripts\").lint()'",
		"format": "node -e 'require(\"./scripts\").format()'",
		"project-setup": "node -e 'require(\"./scripts\").projectSetup()'",
